<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">


	<bean id="dailyMainDaoImpl" class="com.yolo.dailyMain.dao.DailyMainDaoImpl">
		<property name="sqlSessionTemplate" ref="sqlSessionTemplate"></property>
	</bean>
	
	<bean id="dailyMainBizImpl" class="com.yolo.dailyMain.biz.DailyMainBizImpl">
		<property name="dailyMainDao" ref="dailyMainDaoImpl"></property>
	</bean>
	
	<bean id ="dailyMainServiceImpl" class="com.yolo.dailyMain.service.DailyMainServiceImpl">
		<property name="dailyMainBiz" ref="dailyMainBizImpl"></property>
		<property name="likeBiz" ref="likeBizImpl"></property>
	</bean>


</beans>
